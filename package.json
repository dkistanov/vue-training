    "babelify": "^8.0.0",
    "browser-sync": "^2.23.6",
    "browserify": "^16.2.0",
    "cross-env": "^5.1.4",
    "cssnano": "^3.10.0",
    "envify": "^4.1.0",
    "eslint": "^4.19.1",
