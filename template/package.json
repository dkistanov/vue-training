{
  "name": "{{ name }}",
  "author": "{{ author }}",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp build",
    "clean": "gulp clean",
    "format": "prettier --write '**/*.{js,ts,vue,yaml,json,md}'",
    "lint": "clear && vue-tslint && gulp sass:lint",
    "start": "cross-env NODE_ENV=development gulp watch",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/vfile-message": "^2.0.0",
    "autoprefixer": "^9.7.4",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "envify": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-check-filesize": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-noop": "^1.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "jest": "^25.1.0",
    "js-yaml": "^3.13.1",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-image-set-polyfill": "^1.0.0",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.14.2",
    "ts-jest": "^25.1.0",
    "tsify": "^4.0.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslintify": "^2.0.2",
    "typescript": "^3.7.5",
    "uglifyify": "^5.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vue-jest": "^3.0.5",
    "vue-tslint": "^0.3.2",
    "vueify-next": "^9.5.3"
  },
  "dependencies": {
    "reset-css": "^5.0.1",
    "vue": "^2.6.11"
  },
  "browserslist": [
    "last 3 versions",
    "not ie <= 8"
  ],
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard"
    ],
    "rules": {
      "selector-pseudo-element-colon-notation": "single",
      "no-descending-specificity": null,
      "value-list-comma-newline-after": null,
      "declaration-colon-newline-after": null
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 120
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.(js)$": "babel-jest",
      "^.+\\.(ts)$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  }
}
